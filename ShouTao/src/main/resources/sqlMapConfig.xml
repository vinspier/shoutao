<!--
    类型别名是为 Java 类型设置一个短的名字。它只和 XML 配置有关，存在的意义仅在于用来减少类完全限定名的冗余
-->
    <typeAliases>
        <typeAlias type="com.vinspier.pojo.User" alias="user"/>
        <typeAlias type="com.vinspier.pojo.Category" alias="category"/>
        <typeAlias type="com.vinspier.pojo.Product" alias="product"/>
        <typeAlias type="com.vinspier.pojo.Order" alias="order"/>
        <typeAlias type="com.vinspier.pojo.OrderItem" alias="orderItem"/>
        <typeAlias type="com.vinspier.pojo.CartItem" alias="cartItem"/>
        <typeAlias type="com.vinspier.pojo.Role" alias="role"/>
        <typeAlias type="com.vinspier.pojo.Administrator" alias="administrator"/>
        <typeAlias type="com.vinspier.pojo.Suggestion" alias="suggestion"/>
        <typeAlias type="com.vinspier.pojo.Suggestion" alias="suggestion"/>
    </typeAliases>
    <mappers>
        <mapper resource="com/vinspier/daoMapper/UserMapper.xml"/>
        <mapper resource="com/vinspier/daoMapper/CategoryMapper.xml"/>
        <mapper resource="com/vinspier/daoMapper/ProductMapper.xml"/>
        <mapper resource="com/vinspier/daoMapper/OrderMapper.xml"/>
        <mapper resource="com/vinspier/daoMapper/OrderMapper.xml"/>
        <mapper resource="com/vinspier/daoMapper/CartMapper.xml"/>
        <mapper resource="com/vinspier/daoMapper/RoleMapper.xml"/>
        <mapper resource="com/vinspier/daoMapper/AdministratorMapper.xml"/>
        <mapper resource="com/vinspier/daoMapper/UserMapper.xml"/>
        <mapper resource="com/vinspier/daoMapper/AdministratorMapper.xml"/>
    </mappers>
</configuration>